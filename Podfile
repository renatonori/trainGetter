	pod 'FirebaseStorage', :git => 'https://github.com/firebase/firebase-ios-sdk.git'
  	pod 'Kanna'
  	pod 'Alamofire'
  	pod 'ReachabilitySwift'
	pod 'SwiftCSVExport'
end
